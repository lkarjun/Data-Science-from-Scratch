      "source": [
        "def paragraph_mentions(text: str, keyword: str) -> bool:\n",
        "  soup = bs(test, 'html5lib')\n",
        "  paragraphs = [p.get_text() for p in soup('p')]\n",
        "  return any(keyword.lower() in paragraph.lower()\n",
        "            for paragraph in paragraphs)"
      ],
      "execution_count": 39,
      "outputs": []
